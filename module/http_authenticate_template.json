{
  "name": "",
  "module": "HttpAuthenticator",
  "parameter": {
    "url": "",
    "httpMethod": "",
    "header": {
      "token": [""]
    },
    "query": {
      "key": "value"
    },
    "body": {
    },
    "varsAssign": [
      "credentials:header.token"
    ],
